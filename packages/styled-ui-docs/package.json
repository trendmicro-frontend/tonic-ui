{
  "name": "styled-ui-docs",
  "version": "0.17.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production next build",
    "clean": "del build docs",
    "dev": "next dev --hostname 0.0.0.0",
    "postbuild": "next export --outdir docs && touch docs/.nojekyll",
    "pre-push": "eslint --ext .js,.jsx .",
    "start": "next start",
    "test": "jest --passWithNoTests"
  },
  "devDependencies": {
    "@emotion/react": "11.x",
    "@emotion/styled": "11.x",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@next/mdx": "^10.0.7",
    "@trendmicro/react-styled-ui": "^0.17.0",
    "@trendmicro/styled-ui-theme": "^0.13.0",
    "@zeit/next-css": "^1.0.1",
    "boolean": "^3.0.1",
    "cross-env": "~7.0.3",
    "del-cli": "~3.0.1",
    "dotenv-flow": "~3.2.0",
    "ensure-type": "1.x",
    "immutability-helper": "^3.1.1",
    "jest": "^26.6.3",
    "next": "^10.0.7",
    "next-compose-plugins": "^2.2.1",
    "node-emoji": "^1.10.0",
    "prism-react-renderer": "^1.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.8.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.8.0",
    "react-ga": "^3.3.0",
    "react-live": "^2.2.2",
    "react-lorem-component": "^0.13.0",
    "react-movable": "^2.5.1",
    "react-table": "^7.5.2",
    "react-virtualized": "^9.21.2",
    "remark-emoji": "^2.1.0",
    "remark-images": "^2.0.0",
    "toasted-notes": "3.2.x",
    "yiq": "^4.0.0"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ],
    "collectCoverageFrom": [
      "<rootDir>/components/**/*.{js,jsx}",
      "<rootDir>/pages/**/*.{js,jsx}",
      "<rootDir>/shared/**/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
